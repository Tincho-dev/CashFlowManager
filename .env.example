# CashFlowManager Environment Configuration
# Copy this file to .env.local and modify as needed

# Database Configuration
# Set to 'true' for local SQLite (in-browser), 'false' for SQL Server
VITE_IS_LOCAL=true

# SQL Server Connection String (only used when VITE_IS_LOCAL=false)
VITE_DB_CONNECTION_STRING=Server=localhost;Database=Finanzas;Trusted_Connection=True;

# LLM Configuration (only used when VITE_IS_LOCAL=false)
# Provider options: 'chatgpt', 'ollama', 'gemini', 'none'
VITE_LLM_PROVIDER=none

# LLM API Endpoint
# For ChatGPT: https://api.openai.com/v1/chat/completions
# For Ollama: http://localhost:11434
# For Gemini: https://generativelanguage.googleapis.com/v1beta/models
VITE_LLM_API_ENDPOINT=

# LLM API Key (for ChatGPT and Gemini)
VITE_LLM_API_KEY=

# LLM Model Name
# For ChatGPT: gpt-4, gpt-3.5-turbo, etc.
# For Ollama: llama2, codellama, etc.
# For Gemini: gemini-pro, gemini-pro-vision, etc.
VITE_LLM_MODEL=
